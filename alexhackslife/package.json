{
  "name": "tailwind-nextjs-starter-blog",
  "version": "1.1.4",
  "private": true,
  "scripts": {
    "start": "node ./scripts/next-remote-watch.js ./data",
    "dev": "next dev",
    "export": "next export",
    "build": "next build && node ./scripts/generate-sitemap",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install",
    "deploy-staging": "aws s3 sync out/ s3://staging-alexhacks-life/ --delete --exclude *.DS_Store --profile personal",
    "deploy-prod": "aws s3 sync out/ s3://alexhacks.life/ --delete --exclude *.DS_Store --profile personal",
    "deploy-prod-from-staging": "aws s3 sync s3://staging-alexhacks-life/ s3://alexhacks.life/ --delete --exclude *.DS_Store --profile personal"
  },
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.58",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.2.5",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.1.1",
    "lodash": "^4.17.21",
    "mdx-bundler": "^10.0.3",
    "next": "15.0.1",
    "next-themes": "^0.3.0",
    "postcss": "^8.3.5",
    "preact": "^10.5.13",
    "react": "18.3.1",
    "react-device-detect": "^2.1.2",
    "react-dom": "18.3.1",
    "react-image-lightbox": "^5.1.4",
    "react-infinite-scroll-component": "^6.1.0",
    "react-player": "^2.9.0",
    "react-spinners": "^0.14.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-prism-plus": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark-footnotes": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "sharp": "^0.33.5",
    "smoothscroll-polyfill": "^0.4.4",
    "tailwindcss": "^3.4.14",
    "unist-util-visit": "^5.0.0",
    "video.js": "^8.18.1",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "15.0.1",
    "@svgr/webpack": "^8.1.0",
    "aws-sdk": "^2.1013.0",
    "cross-env": "^7.0.3",
    "dedent": "^1.5.3",
    "eslint": "^9.13.0",
    "eslint-config-next": "15.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^5.2.1",
    "file-loader": "^6.0.0",
    "globby": "14.0.2",
    "husky": "^9.1.6",
    "inquirer": "^12.0.0",
    "lint-staged": "^15.2.10",
    "next-remote-watch": "^2.0.0",
    "prettier": "3.3.3",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
